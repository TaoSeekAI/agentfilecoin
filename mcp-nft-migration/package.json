{
  "name": "mcp-nft-migration",
  "version": "1.0.0",
  "description": "MCP Server for NFT IPFS to Filecoin Migration",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "mcp-nft-migration": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/index.js && chmod +x build/index-daemon.js",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "dev": "tsc && node build/index.js",
    "daemon": "node build/index-daemon.js"
  },
  "keywords": [
    "mcp",
    "nft",
    "filecoin",
    "ipfs",
    "migration",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
